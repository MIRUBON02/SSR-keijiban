    "@vitejs/plugin-react-swc": "^3.11.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.30.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
